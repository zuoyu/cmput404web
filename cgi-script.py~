#!/usr/bin/env python

import os,json, cgi

print "Content-type: text/html"
print "Location: http://google.com/"
print
print "<HTML><BODY><H1>HELLO,WORLD!</H1>" 
print "<FORM method ='POST'><INPUT name ='x'></FORM>"
form = cgi.FieldStorage()


print "<P>X was" + str(form.getvalue('x'))
